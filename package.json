{
  "name": "configcat-js-ssr",
  "version": "5.6.1",
  "description": "ConfigCat Feature Flags for Server Side Rendered apps like NuxtJS. Official ConfigCat SDK for Server Side Rendered to easily access feature flags.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/esm/index.js",
  "scripts": {
    "test": "karma start karma-chrome-coverage.conf.js",
    "test-firefox": "karma start karma-firefox.conf.js",
    "test-chrome": "karma start karma-chrome.conf.js",
    "test-chromium": "karma start karma-chromium.conf.js",
    "build": "tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json && gulp tsc",
    "buildPure": "webpack && gulp webpack",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/configcat/nuxt-sdk.git"
  },
  "keywords": [
    "feature flags",
    "feature flags",
    "configcat",
    "nuxtjs",
    "vue",
    "ssr",
    "config",
    "configuration",
    "remote configuration",
    "configcat client",
    "feature flags",
    "feature toggle",
    "feature switch",
    "canary release",
    "soft launch",
    "release strategy"
  ],
  "author": "ConfigCat",
  "license": "MIT",
  "homepage": "https://configcat.com",
  "bugs": {
    "url": "https://github.com/configcat/nuxt-sdk/issues"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "configcat-common": "^7.0.1",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.5",
    "chai": "^4.3.6",
    "core-js": "^3.24.1",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.3",
    "gulp-typescript": "^6.0.0-alpha.1",
    "karma": "^6.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.3",
    "mocha": "^8.1.0",
    "ts-loader": "^9.3.1",
    "typescript": "^3.9.10",
    "webpack": "^5.74.0",
    "webpack-auto-inject-version": "^1.2.2",
    "webpack-cli": "^4.10.0"
  }
}
